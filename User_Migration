
Schema::create('users', function (Blueprint $table) {
            $table->bigIncrements('id');
            $table->string('service_id');
            $table->string('grant_type');
            $table->text('access_token');
            $table->text('refresh_token');
            $table->timestamp('token_expires_at');
            $table->rememberToken();
            $table->timestamps();
        });
